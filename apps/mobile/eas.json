{
    "cli": {
        "appVersionSource": "remote"
    },
    "build": {
        "production": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_BASE_API_URL": "https://vphim-api.giaang.id.vn",
                "EXPO_PUBLIC_BASE_PLAYER_URL": "https://vephim.vercel.app"
            }
        },
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "simulator": true
            },
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_BASE_API_URL": "https://vphim-api.giaang.id.vn",
                "EXPO_PUBLIC_BASE_PLAYER_URL": "https://vephim.vercel.app"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
